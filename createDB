create database myHealthData default character set utf8 collate utf8_general_ci;

create table my_health_data(
    key_date date,
    diastolic float,
    systolic float,
    pulse float,
    weight float,
    diastolic_1 float,
    diastolic_2 float,
    diastolic_3 float,
    diastolic_4 float,
    diastolic_5 float,
    diastolic_6 float,
    systolic_1 float,
    systolic_2 float,
    systolic_3 float,
    systolic_4 float,
    systolic_5 float,
    systolic_6 float,
    pulse_1 float,
    pulse_2 float,
    pulse_3 float,
    pulse_4 float,
    pulse_5 float,
    pulse_6 float,
    PRIMARY KEY(key_date)
);